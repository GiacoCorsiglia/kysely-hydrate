{
	"name": "kysely-hydrate",
	"version": "0.7.2",
	"description": "Explicit ORM-style queries with Kysely",
	"homepage": "https://github.com/GiacoCorsiglia/kysely-hydrate#readme",
	"bugs": {
		"url": "https://github.com/GiacoCorsiglia/kysely-hydrate/issues"
	},
	"license": "MIT",
	"author": {
		"name": "Giaco Corsiglia",
		"url": "https://giacocorsiglia.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GiacoCorsiglia/kysely-hydrate.git"
	},
	"files": [
		"dist"
	],
	"type": "module",
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": "./dist/index.mjs",
		"./experimental": "./dist/experimental.mjs",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "tsdown",
		"release": "./scripts/release.sh",
		"test": "node --test",
		"test:sqlite": "HYDRATE_TEST_DB=sqlite npm run test",
		"test:postgres": "HYDRATE_TEST_DB=postgres npm run test",
		"test:all": "npm run test:sqlite && npm run test:postgres",
		"typecheck": "tsgo",
		"typecheck:tsc": "tsc",
		"lint": "oxlint",
		"format": "oxfmt",
		"prepublishOnly": "npm run build"
	},
	"devDependencies": {
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^22",
		"@types/pg": "^8.16.0",
		"@typescript/native-preview": "^7.0.0-dev.20260108.1",
		"better-sqlite3": "^12.4.1",
		"expect-type": "^1.3.0",
		"kysely": "^0.28.7",
		"oxfmt": "^0.23.0",
		"oxlint": "^1.38.0",
		"pg": "^8.16.3",
		"postgres-array": "^3.0.4",
		"tsdown": "^0.17.1",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"kysely": "^0.28"
	},
	"engines": {
		"node": ">=22.18.0"
